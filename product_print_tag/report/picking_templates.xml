<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <template id="stock.label_transfer_template_view_pdf">
           <t t-call="web.basic_layout">
                <div>
                    <t t-foreach="docs" t-as="picking">
                        <t t-foreach="picking.move_lines" t-as="move">
                            <t t-foreach="move.move_line_ids" t-as="move_line">
                                <t t-if="move_line.product_uom_id.category_id.measure_type == 'unit'">
                                    <t t-set="qty" t-value="int(move_line.qty_done)"/>
                                </t>
                                <t t-else="">
                                    <t t-set="qty" t-value="1"/>
                                </t>
                                <t t-foreach="range(qty)" t-as="item">
                                    <t t-translation="off">
                                        <div class="" style="width:840px;">
                                            <table class="table" t-if="picking.move_lines" style="">
                                              
                                                <tr style="padding:0px !important;margin:0px !important;border:0px !important; margin-top:16px !important;" >
                                                  
                                                  <td class="w-10" style="border:0px;">
                                                  </td>
                                                  
                                                    <td class="text-center w-40" style="font-size:14px;font-weight:bold;line-height:16px;background:none;padding:0px;margin:0px; border:0px;" >
                                                      <br/>
                                                      <span t-esc="move.product_id.default_code"/>
                                                    </td>
                                                    
                                                  <td class="w-10" style="border:0px;">
                                                  </td>
                                                    
                                                    <td class="text-center w-40" style="font-size:14px;font-weight:bold;line-height:16px;background:none;padding:0px;margin:0px; border:0px;" >
                                                      <br/>
                                                      <span t-esc="move.product_id.default_code"/>
                                                    </td>
                                                    
                                                </tr>
                                                
                                                <t t-if="move_line.product_id.tracking != 'none'"> 
                                                     <tr style="padding:0px !important;margin:0px !important;border:0px !important" >
                                                        <td class="w-10" align="center" style="padding:0px; border:0px !important">
                                                            <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                                  <t t-if="attribute.attribute_id.display_type == 'size'">
                                                                      <br/>
                                                                      <strong><span t-esc="attribute.name" style="font-size:28px;"/></strong>
                                                                      <br/>
                                                                  </t>
                                                            </t>
                                                            <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                                  <t t-if="attribute.attribute_id.display_type == 'color'">
                                                                      <strong><span t-esc="attribute.name" style="font-size:24px;"/></strong>
                                                                  </t>
                                                            </t>
                                                        </td>    
                                                        <td class="w-40 text-center" style="border:0px;"> 
                                                            <t t-if="move_line.product_id.barcode">
                                                                <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', move_line.product_id.barcode, 290, 100)" style="width:290px;height:100px;" alt="Barcode"/>
                                                            </t>
                                                        </td>
                                                        <td class="w-10" align="center" style="padding:0px;border:0px; ">
                                                          <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                            <t t-if="attribute.attribute_id.display_type == 'size'">
                                                              <br/>
                                                              <strong><span t-esc="attribute.name" style="font-size:28px;"/></strong>
                                                              <br/>
                                                            </t>
                                                          </t>
                                                          <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                            <t t-if="attribute.attribute_id.display_type == 'color'">
                                                              <strong><span t-esc="attribute.name" style="font-size:24px;"/></strong>
                                                            
                                                            </t>
                                                          </t>
                                                        </td>

                                                        <td class="w-40 text-center" style="border:0px;"> 
                                                            <t t-if="move_line.product_id.barcode">
                                                                <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', move_line.product_id.barcode, 290, 100)" style="width:290px;height:100px;" alt="Barcode"/>
                                                            </t>
                                                        </td>
                                                    </tr>
                                                    
                                                    
                                                    <tr style="padding:0px !important;margin:0px !important;border:0px !important" >
                                                        <td class="w-10" align="center" style="border:0px;">
                                                          
                                                        </td>    
                                                        <td class="w-40 text-center" style="border:0px;">   
                                                            <t t-if="move_line.lot_name or move_line.lot_id">
                                                                <span t-esc="move_line.lot_name or move_line.lot_id.name" style="font-size:2rem;"/>
                                                            </t>
                                                            <t t-else="">
                                                                <span class="text-muted" style="font-size:12px;">No barcode available</span>
                                                            </t>
                                                        </td>
                                                        
                                                        <td class="w-10" style="border:0px;">
                                                        </td>

                                                        <td class="w-40 text-center" style="border:0px;"> 
                                                            <t t-if="move_line.lot_name or move_line.lot_id">

                                                                <span t-esc="move_line.lot_name or move_line.lot_id.name" style="font-size:2rem;"/>
                                                            </t>
                                                            <t t-else="">
                                                                <span class="text-muted" style="font-size:12px;">No barcode available</span>
                                                            </t>
                                                        </td>
                                                    </tr>
                                                    
                                                </t>           

                                                
                                                
                                                <t t-if="move_line.product_id.tracking == 'none'">
                                                    <tr style="padding:0px !important;margin:0px !important;border:0px !important;">
                                                        <td class="w-10" align="center" style="padding:0px; border:0px !important;">
                                                            <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                                  <t t-if="attribute.attribute_id.display_type == 'size'">
                                                                      <br/>
                                                                      <strong><span t-esc="attribute.name" style="font-size:28px;"/></strong>
                                                                      <br/>
                                                                  </t>
                                                            </t>
                                                            <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                                  <t t-if="attribute.attribute_id.display_type == 'color'">
                                                                      <strong><span t-esc="attribute.name" style="font-size:24px;"/></strong>
                                                                  </t>
                                                            </t>
                                                        </td>    
                                                        <td class="w-40 text-center" style="border:0px;"> 
                                                            <t t-if="move_line.product_id.barcode">
                                                                <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', move_line.product_id.barcode, 290, 100)" style="width:290px;height:100px;" alt="Barcode"/>
                                                            </t>
                                                        </td>
                                                        <td class="w-10" align="center" style="padding:0px;border:0px; ">
                                                          <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                            <t t-if="attribute.attribute_id.display_type == 'size'">
                                                              <br/>
                                                              <strong><span t-esc="attribute.name" style="font-size:28px;"/></strong>
                                                              <br/>
                                                            </t>
                                                          </t>
                                                          <t t-foreach="move_line.product_id.product_template_attribute_value_ids" t-as="attribute">
                                                            <t t-if="attribute.attribute_id.display_type == 'color'">
                                                              <strong><span t-esc="attribute.name" style="font-size:24px;"/></strong>
                                                            
                                                            </t>
                                                          </t>
                                                        </td>

                                                        <td class="w-40 text-center" style="border:0px;"> 
                                                            <t t-if="move_line.product_id.barcode">
                                                                <img t-att-src="'/report/barcode/?type=%s&amp;value=%s&amp;width=%s&amp;height=%s' % ('Code128', move_line.product_id.barcode, 290, 100)" style="width:290px;height:100px;" alt="Barcode"/>
                                                            </t>
                                                        </td>
                                                    </tr>
                                                    
                                                    
                                                    <tr style="padding:0px !important;margin:0px !important;border:0px !important" >
                                                        <td class="w-10" align="center" style="border:0px;">
                                                          
                                                        </td>    
                                                        <td class="w-40 text-center" style="border:0px;">   
                                                            <t t-if="move_line.product_id.barcode">
                                                                <span t-esc="move_line.product_id.barcode" style="font-size:12px;"/>
                                                            </t>
                                                            <t t-else="">
                                                                <span class="text-muted" style="font-size:12px;">No barcode available</span>
                                                            </t>
                                                        </td>
                                                        
                                                        <td class="w-10" style="border:0px;">
                                                        </td>

                                                        <td class="w-40 text-center" style="border:0px;"> 
                                                            <t t-if="move_line.product_id.barcode">
                                                                <span t-esc="move_line.product_id.barcode" style="font-size:12px;"/>
                                                            </t>
                                                            <t t-else="">
                                                                <span class="text-muted" style="font-size:12px;">No barcode available</span>
                                                            </t>
                                                        </td>
                                                    </tr>
                                                    
                                                </t>
                                            </table>
                                        </div>
                                        <p style="page-break-before:always;"> </p>
                                    </t>
                                </t>
                            </t>
                        </t>
                    </t>
                </div>
            </t>
        </template>
        <!--comentario-->
    </data>
</odoo>
